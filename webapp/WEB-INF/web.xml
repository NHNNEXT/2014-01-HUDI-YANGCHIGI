<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
  
	<servlet>
		<servlet-name>next</servlet-name>
		<servlet-class>org.yangchigi.web.DefaultServlet</servlet-class>
	</servlet>
	
	<servlet>	
		<servlet-name>landing</servlet-name>
		<servlet-class>org.yangchigi.web.LandingServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>next</servlet-name>
		<url-pattern>/aa</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>	
		<servlet-name>landing</servlet-name>
		<url-pattern>/seize</url-pattern>
	</servlet-mapping>
	
</web-app>
