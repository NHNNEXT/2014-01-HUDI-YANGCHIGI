<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:javaee="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <javaee:display-name>Archetype Created Web Application</javaee:display-name>
  <servlet>
    <servlet-name>landing</servlet-name>
    <servlet-class>org.yangchigi.web.LandingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>landing</servlet-name>
    <url-pattern>/landing</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>SignUpServlet</servlet-name>
    <servlet-class>org.yangchigi.web.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUpServlet</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>MyPageServlet</servlet-name>
    <servlet-class>org.yangchigi.web.MyPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyPageServlet</servlet-name>
    <url-pattern>/mypage</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>WriteArticleServlet</servlet-name>
    <servlet-class>org.yangchigi.web.WriteArticleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WriteArticleServlet</servlet-name>
    <url-pattern>/writearticle</url-pattern>
  </servlet-mapping>
</web-app>