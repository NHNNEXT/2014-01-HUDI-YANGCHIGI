<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:javaee="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<javaee:display-name>Archetype Created Web Application
	</javaee:display-name>

	<servlet>
		<servlet-name>LandingServlet</servlet-name>
		<servlet-class>org.yangchigi.web.LandingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LandingServlet</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>org.yangchigi.web.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/user/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MyPageServlet</servlet-name>
		<servlet-class>org.yangchigi.web.MyPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyPageServlet</servlet-name>
		<url-pattern>/mypage/*</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>TodayServlet</servlet-name>
		<servlet-class>org.yangchigi.web.TodayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TodayServlet</servlet-name>
		<url-pattern>/today/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserModifyServlet</servlet-name>
		<servlet-class>org.yangchigi.web.UserModifyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserModifyServlet</servlet-name>
		<url-pattern>/usermodify/*</url-pattern>
	</servlet-mapping>



